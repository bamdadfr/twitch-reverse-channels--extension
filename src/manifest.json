{
  "name": "Twitch Reverse Channels",
  "short_name": "twitch-reverse-channels--extension",
  "description": "Show channels in reverse order for Twitch",
  "version": "1.0.15",
  "manifest_version": 2,
  "icons": {
    "512": "assets/icon-386.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.twitch.tv/*"
      ],
      "js": [
        "scripts/content.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "permissions": [
    "*://*.twitch.tv/*"
  ]
}
